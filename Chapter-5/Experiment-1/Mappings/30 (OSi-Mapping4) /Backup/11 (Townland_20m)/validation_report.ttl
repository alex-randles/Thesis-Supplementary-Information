@prefix ex: <http://example.org/> .
@prefix mqv: <https://w3id.org/MQIO#> .
@prefix mqv-metric: <https://w3id.org/MQIO-metrics#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ont: <http://ontologies.geohive.ie/osi#> .


ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <Townland_20m.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

<Townland_20m.ttl> a mqv:MappingArtefact .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
                     ex:violation-1;
     .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLiteralValue "ga" ;
    mqv:hasLocation "predicateObjectMap3"^^xsd:string ;
    mqv:inTripleMap <#TriplesMap1> ;
    mqv:isDescribedBy mqv-metric:MP6 ;
    mqv:resultMessage "Language tag not defined in RFC 5646."^^xsd:string .

ex:violation-1 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap4" ;
    mqv:hasObjectValue ont:logainmId ;
    mqv:inTripleMap <#TriplesMap1> ;
    mqv:isDescribedBy  mqv-metric:D2 ;
    mqv:resultMessage "Usage of undefined Property." ;
    mqv:wasRefinedBy ex:refinement-1 .
