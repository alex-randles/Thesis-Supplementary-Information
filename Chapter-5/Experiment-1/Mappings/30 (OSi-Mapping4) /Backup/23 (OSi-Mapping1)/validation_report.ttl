@prefix daq: <http://purl.org/eis/vocab/daq#> .
@prefix ex: <http://example.org/> .
@prefix mqv: <https://w3id.org/MQIO#> .
@prefix mqv-metric: <https://w3id.org/MQIO-metrics#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix sdmx-dimension:    <http://purl.org/linked-data/sdmx/2009/dimension#>.

ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <mappingDAQobservation_2nd_Dump.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
        ex:violation-1,
        ex:violation-2,
        ex:violation-3 .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap9"^^xsd:string ;
    mqv:hasObjectValue prov:generated ;
    mqv:inTripleMap <#Observations> ;
    mqv:isDescribedBy mqv-metric:D3 ;
    mqv:resultMessage "Usage of incorrect domain."^^xsd:string .

ex:violation-1 a mqv:MappingViolation ;
    mqv:hasLocation "subjectMap"^^xsd:string ;
    mqv:hasObjectValue daq:MetricProfile ;
    mqv:inTripleMap <#Profiling> ;
    mqv:isDescribedBy mqv-metric:D1 ;
    mqv:resultMessage "Usage of undefined Class."^^xsd:string .

ex:violation-2 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue daq:totalDatasetTriplesAssessed ;
    mqv:inTripleMap <#Profiling> ;
    mqv:isDescribedBy mqv-metric:D2 ;
    mqv:resultMessage "Usage of undefined Property."^^xsd:string .

ex:violation-3 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap-4"^^xsd:string ;
    mqv:hasObjectValue sdmx-dimension:timePeriod ;
    mqv:hasResultMessage "Usage of incorrect range. Term type should be  rr:IRI  for property  sdmx-dimension:timePeriod ."^^xsd:string ;
    mqv:inTripleMap <#Observations> ;
    mqv:isDescribedBy mqv-metric:D6 .

<mappingDAQobservation_2nd_Dump.ttl> a mqv:MappingArtefact .

