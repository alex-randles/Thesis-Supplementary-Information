        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bitter</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#breakfast -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#breakfast">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#MealTime"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The first meal of the day</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">breakfast</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#cold -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#cold">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#Temperature"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">At a low temperature, especially when compared to the temperature of the human body, and not hot, or warm</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cold</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#dinner -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#dinner">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#MealTime"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The main meal of the day, eaten either in the middle of the day or in the evening</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dinner</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#grams -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#grams">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#UnitMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A unit of mass equal to 0.001 kilograms</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">grams</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#kilograms -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#kilograms">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#UnitMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A unit of mass equal to 1000 grams</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">kilograms</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#liters -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#liters">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#UnitMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A unit for measuring the volume of a liquid or gas, equal to 1000 cubic centimeters</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">liters</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#lunch -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#lunch">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#MealTime"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A meal eaten in the middle of the day</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">lunch</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#oven -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#oven">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#CookingTool"/>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#salty -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#salty">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#Flavor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Containing or tasting of salt</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">salty</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#snack -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#snack">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#MealTime"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A small meal or amount of food, usually eaten in a hurry</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">snack</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#sour -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#sour">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#Flavor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Having a taste like that of a lemon or of fruit that is not ready to eat</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sour</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#spicy -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#spicy">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#Flavor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Having a strong taste because spices have been used to flavor it</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">spicy</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#spring -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#spring">
        <rdf:type rdf:resource="http://ontology.wearehungry.be#Season"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The season of the year between winter and summer, lasting from March to June north of the equator, and from September to December south of the equator, when the weather becomes warmer, leaves and plants start to grow again and flowers appear</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">spring</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#summer -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#summer">
