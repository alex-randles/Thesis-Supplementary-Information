    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This rule is used to calculate total price of a recipe using the price of each ingredient.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TotalPrice</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://ontology.wearehungry.be#Recipe"/>
                        <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#r"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                <swrl:classPredicate rdf:resource="http://ontology.wearehungry.be#AmountIngredient"/>
                                <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#ai"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="http://ontology.wearehungry.be#usesIngredient"/>
                                        <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#ai"/>
                                        <swrl:argument2 rdf:resource="http://ontology.wearehungry.be#ingredient"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://ontology.wearehungry.be#hasUnit"/>
                                                <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#ai"/>
                                                <swrl:argument2 rdf:resource="http://ontology.wearehungry.be#aiUnit"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                                        <swrl:propertyPredicate rdf:resource="http://ontology.wearehungry.be#unitValue"/>
                                                        <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#aiUnit"/>
                                                        <swrl:argument2 rdf:resource="http://ontology.wearehungry.be#amountIngredientUsed"/>
                                                    </rdf:Description>
                                                </rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                        <rdf:first>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                                <swrl:propertyPredicate rdf:resource="http://ontology.wearehungry.be#costs"/>
                                                                <swrl:argument1 rdf:resource="http://ontology.wearehungry.be#ingredient"/>
                                                                <swrl:argument2 rdf:resource="http://ontology.wearehungry.be#price"/>
                                                            </rdf:Description>
                                                        </rdf:first>
