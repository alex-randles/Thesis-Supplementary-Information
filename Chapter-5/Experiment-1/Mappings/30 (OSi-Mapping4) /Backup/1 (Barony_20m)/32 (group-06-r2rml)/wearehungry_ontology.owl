        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">autumn</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontology.wearehungry.be#bitter -->

    <owl:NamedIndividual rdf:about="http://ontology.wearehungry.be#bitter">
