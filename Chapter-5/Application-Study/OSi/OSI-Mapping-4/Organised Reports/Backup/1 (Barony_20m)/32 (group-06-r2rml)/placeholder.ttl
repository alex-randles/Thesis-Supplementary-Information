ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <recipes-mapping.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
        ex:violation-1,
        ex:violation-10,
        ex:violation-11,
        ex:violation-12,
        ex:violation-13,
        ex:violation-14,
        ex:violation-15,
        ex:violation-16,
        ex:violation-17,
        ex:violation-2,
        ex:violation-3,
        ex:violation-4,
        ex:violation-5,
        ex:violation-6,
        ex:violation-7,
        ex:violation-8,
        ex:violation-9 .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue recipes:contains ;
    mqv:inTripleMap <#RecipeContainsAmountIngredientProperty> ;
    mqv:isDescribedBy mqv-metric:D3 ;
    mqv:resultMessage "Usage of incorrect domain."^^xsd:string .

