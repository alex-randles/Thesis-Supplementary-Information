@prefix ex: <http://example.org/> .
@prefix mqv: <https://w3id.org/MQIO#> .
@prefix mqv-metric: <https://w3id.org/MQIO-metrics#> .
@prefix recipes: <http://ontology.foodlog.eu#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <recipes-mapping.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
        ex:violation-1,
        ex:violation-2 .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue recipes:stepNumber ;
    mqv:inTripleMap <#RecipeStepNoIng> ;
    mqv:isDescribedBy mqv-metric:D7 ;
    mqv:resultMessage "Usage of incorrect datatype."^^xsd:string .

ex:violation-1 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap5"^^xsd:string ;
    mqv:hasObjectValue recipes:serving ;
    mqv:inTripleMap <#Recipe> ;
    mqv:isDescribedBy mqv-metric:D7 ;
    mqv:resultMessage "Usage of incorrect datatype."^^xsd:string .

ex:violation-2 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue recipes:stepNumber ;
    mqv:inTripleMap <#RecipeStep> ;
    mqv:isDescribedBy mqv-metric:D7 ;
    mqv:resultMessage "Usage of incorrect datatype."^^xsd:string .

<recipes-mapping.ttl> a mqv:MappingArtefact .

