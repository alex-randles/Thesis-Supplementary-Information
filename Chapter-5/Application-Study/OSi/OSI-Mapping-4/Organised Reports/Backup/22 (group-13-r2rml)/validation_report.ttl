@prefix ex: <http://example.org/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix mqv: <https://w3id.org/MQIO#> .
@prefix mqv-metric: <https://w3id.org/MQIO-metrics#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <r2rml.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
        ex:violation-1,
        ex:violation-2, 
        ex:violation-3 .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue geo:long ;
    mqv:inTripleMap <#PointMap> ;
    mqv:isDescribedBy mqv-metric:D3 ;
    mqv:resultMessage "Usage of incorrect domain."^^xsd:string .

ex:violation-1 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap2"^^xsd:string ;
    mqv:hasObjectValue geo:lat ;
    mqv:inTripleMap <#PointMap> ;
    mqv:isDescribedBy mqv-metric:D3 ;
    mqv:resultMessage "Usage of incorrect domain."^^xsd:string .

ex:violation-2 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap1"^^xsd:string ;
    mqv:hasObjectValue time:seconds ;
    mqv:inTripleMap <#StepDurationMap> ;
    mqv:isDescribedBy mqv-metric:D3 ;
    mqv:resultMessage "Usage of incorrect domain."^^xsd:string .

ex:violation-3 a mqv:MappingViolation ;
    mqv:hasLocation "subjectMap"^^xsd:string ;
    mqv:hasObjectValue geo:Point ;
    mqv:inTripleMap <#PointMap> ;
    mqv:isDescribedBy mqv-metric:D1 ;
    mqv:resultMessage "Usage of undefined Class."^^xsd:string .
    
<r2rml.ttl> a mqv:MappingArtefact .

