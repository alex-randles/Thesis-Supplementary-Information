@prefix ex: <http://example.org/> .
@prefix foodreport: <http://www.foodreport.be/ontology#> .
@prefix mqv: <https://w3id.org/MQIO#> .
@prefix mqv-metric: <https://w3id.org/MQIO-metrics#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <foodreport-mapping.ttl> ;
    mqv:hasValidationReport ex:mappingValidationReport-0 .

<foodreport-mapping.ttl> a mqv:MappingArtefact .

ex:mappingValidationReport-0 a mqv:MappingValidationReport ;
    mqv:hasViolation ex:violation-0,
        ex:violation-1,
        ex:violation-2 .

ex:violation-0 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap10"^^xsd:string ;
    mqv:hasObjectValue foodreport:manufacturedFrom ;
    mqv:inTripleMap <#Ingredient> ;
    mqv:isDescribedBy mqv-metric:D2 ;
    mqv:resultMessage "Usage of undefined Property."^^xsd:string .

ex:violation-1 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap2"^^xsd:string ;
    mqv:hasObjectValue foodreport:lawText ;
    mqv:inTripleMap <#Law> ;
    mqv:isDescribedBy mqv-metric:D2 ;
    mqv:resultMessage "Usage of undefined Property."^^xsd:string .

ex:violation-2 a mqv:MappingViolation ;
    mqv:hasLocation "predicateObjectMap6"^^xsd:string ;
    mqv:hasObjectValue foodreport:appliesToIngredient ;
    mqv:inTripleMap <#Law> ;
    mqv:isDescribedBy mqv-metric:D2 ;
    mqv:resultMessage "Usage of undefined Property."^^xsd:string .

